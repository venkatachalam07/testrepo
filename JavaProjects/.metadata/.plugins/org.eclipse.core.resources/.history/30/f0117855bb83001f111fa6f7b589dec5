package JavaPractice;

public class Mobiles {
	
	public static void main(String[] args) {
		
		
		int price =25000;
		
		
	String	result = "Mobile";
	
	
	
	
	if(price<=20000 && result=="Mobile") {
		
		System.out.println("You Mobile is REDMI");
		
		
	}
	
	else if((price>=21000 && price <=24000 )&& result=="Mobile") {
		
		System.out.println("You Mobile is VIVO");
		
		
	}
	

	else if((price>=20000 && price<=30000 )&& result=="Mobile") {
		
		System.out.println("You Mobile is Moto");
		
		
	}
		
	else {
		System.out.println("you Try can Try EMI option lessthan 5K");

	}
		
		
		
	}
}
