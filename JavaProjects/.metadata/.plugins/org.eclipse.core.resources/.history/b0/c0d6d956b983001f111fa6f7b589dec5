package JavaPractice;

public class DecisionMarks {
	public static void main(String[] args) {
		
		
		// marks & groups
		
		
		
		
		
int Mark =67;


String result = "You Group is This";
		
		if((Mark>=45 && Mark<=60) && result=="You Group is This") {

System.out.println("You Eligible for Commerce");
		}
		
		else if((Mark>=60 && Mark<=75) && result=="You Group is This") {
		
			System.out.println("You Eligible for Computer Science");

		}
		
else {
	
	System.out.println("You not Eligible 11 stanted ");

	
}
	}
	


}
