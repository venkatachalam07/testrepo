package JavaPractice;

public class DecisionColours {
	
	public static void main(String[] args) {
		
		// colours 
		
		String Color= "RED";
		
		
		 String result ="PASS";
		


		 
		 if(Color==result){
			 System.out.println("You Give color is RED");
		 }
		 
		 
		 
		 if(Color!=result){
			 System.out.println("You Give color is RED");
		 }
		 
		 
		 
		 else {
			System.out.println("Your Given color is Wrong ");
		}
		 
		
		
		
	}
	

}
