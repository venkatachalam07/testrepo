package Class;

public class GreaterThreeNumber {
	
	
	public static void main(String[] args) {
		
		
		
		int a=10;
	
		int b=400;
	
	    int c=30000;
	
String  result ="C is greter than";
	
	
	
	
	if(a>b && a>c) {
		
	System.out.println("A is big number");
		
		
	}
	
	else if(b>a && b>c){
		
		System.out.println("B is big number");
		
		
	}
	else {
		System.out.println("C is Bignumber");
		
	}
	
	
		
	}

}
