package InterviewPro;

import java.util.Scanner;


public class pprr {
	
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the Number");
		int num = sc.nextInt();
		
		
		int i=num-1;
		
		while (i>0) {
			
		num=num*i;
			
		

		}
		
		
		
		System.out.println(num);
		System.out.println(i);
		
	}

}
