package InterviewPro;

import java.util.Scanner;

public class smilpeProgram {
	
	
	private void voteeligiblelist() {

		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter You Age");
		
		
		int age = sc.nextInt();
		
		
		if (age>18) {
			
			System.out.println("You eligible for vote");
	      		}
		
		else {
			System.out.println("You Not Eligible for Vote");
		}
		
		
	}
	
	
	public static void main(String[] args) {
		
		voteeligiblelist v=new voteeligiblelist();
		
		
		
	 }
	}
